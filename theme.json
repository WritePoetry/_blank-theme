{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [ "page", "post" ]
		},
		{
			"name": "page-large-header",
			"title": "Page (Large Header)",
			"postTypes": [ "page" ]
		},
		{
			"name": "single-no-separators",
			"title": "Single Post (No Separators)",
			"postTypes": [ "post" ]
		},
		{
			"name": "page-no-separators",
			"title": "Page (No Separators)",
			"postTypes": [ "page" ]
		},
		{
			"name": "blog-alternative",
			"postTypes": [ "page" ],
			"title": "Blog (Alternative)"
		},
		{
			"name": "404",
			"postTypes": [ "page" ],
			"title": "404"
		},
		{
			"name": "page-no-title",
			"postTypes": [ "page" ],
			"title": "Page No Title"
		},
		{
			"name": "page-with-sidebar",
			"postTypes": [ "post", "page" ],
			"title": "Page with Sidebar"
		},
		{
			"name": "page-wide",
			"postTypes": [ "page" ],
			"title": "Page with Wide Image"
		},
		{
			"name": "single-with-sidebar",
			"postTypes": [ "post" ],
			"title": "Single with Sidebar"
		},
		{
			"name": "page-blank",
			"postTypes": [ "page", "post" ],
			"title": "Blank Page"
		}
	],
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"background": true,
			"button": true,
			"caption": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"duotone": [
				{
					"name": "Dark grayscale",
					"colors": [ "#000000", "#7f7f7f" ],
					"slug": "dark-grayscale"
				},
				{
					"name": "Orange and gray",
					"colors": [ "#FA6900", "#EEEEEE" ],
					"slug": "orange-gray"
				},
				{
					"name": "Grayscale",
					"colors": [ "#000000", "#ffffff" ],
					"slug": "grayscale"
				},
				{
					"name": "Purple and yellow",
					"colors": [ "#8c00b7", "#fcff41" ],
					"slug": "purple-yellow"
				},
				{
					"name": "Blue and red",
					"colors": [ "#000097", "#ff4747" ],
					"slug": "blue-red"
				},
				{
					"name": "Midnight",
					"colors": [ "#000000", "#00a5ff" ],
					"slug": "midnight"
				},
				{
					"name": "Magenta and yellow",
					"colors": [ "#c7005a", "#fff278" ],
					"slug": "magenta-yellow"
				},
				{
					"name": "Purple and green",
					"colors": [ "#a60072", "#67ff66" ],
					"slug": "purple-green"
				},
				{
					"name": "Blue and orange",
					"colors": [ "#1900d8", "#ffa96b" ],
					"slug": "blue-orange"
				}
			],
			"gradients": [
				{
					"name": "Vivid cyan blue to vivid purple",
					"gradient": "linear-gradient(135deg,rgba(6,147,227,1) 0%,rgb(155,81,224) 100%)",
					"slug": "vivid-cyan-blue-to-vivid-purple"
				},
				{
					"name": "Light green cyan to vivid green cyan",
					"gradient": "linear-gradient(135deg,rgb(122,220,180) 0%,rgb(0,208,130) 100%)",
					"slug": "light-green-cyan-to-vivid-green-cyan"
				},
				{
					"name": "Luminous vivid amber to luminous vivid orange",
					"gradient": "linear-gradient(135deg,rgba(252,185,0,1) 0%,rgba(255,105,0,1) 100%)",
					"slug": "luminous-vivid-amber-to-luminous-vivid-orange"
				},
				{
					"name": "Luminous vivid orange to vivid red",
					"gradient": "linear-gradient(135deg,rgba(255,105,0,1) 0%,rgb(207,46,46) 100%)",
					"slug": "luminous-vivid-orange-to-vivid-red"
				},
				{
					"name": "Very light gray to cyan bluish gray",
					"gradient": "linear-gradient(135deg,rgb(238,238,238) 0%,rgb(169,184,195) 100%)",
					"slug": "very-light-gray-to-cyan-bluish-gray"
				},
				{
					"name": "Cool to warm spectrum",
					"gradient": "linear-gradient(135deg,rgb(74,234,220) 0%,rgb(151,120,209) 20%,rgb(207,42,186) 40%,rgb(238,44,130) 60%,rgb(251,105,98) 80%,rgb(254,248,76) 100%)",
					"slug": "cool-to-warm-spectrum"
				},
				{
					"name": "Blush light purple",
					"gradient": "linear-gradient(135deg,rgb(255,206,236) 0%,rgb(152,150,240) 100%)",
					"slug": "blush-light-purple"
				},
				{
					"name": "Blush bordeaux",
					"gradient": "linear-gradient(135deg,rgb(254,205,165) 0%,rgb(254,45,45) 50%,rgb(107,0,62) 100%)",
					"slug": "blush-bordeaux"
				},
				{
					"name": "Luminous dusk",
					"gradient": "linear-gradient(135deg,rgb(255,203,112) 0%,rgb(199,81,192) 50%,rgb(65,88,208) 100%)",
					"slug": "luminous-dusk"
				},
				{
					"name": "Pale ocean",
					"gradient": "linear-gradient(135deg,rgb(255,245,203) 0%,rgb(182,227,212) 50%,rgb(51,167,181) 100%)",
					"slug": "pale-ocean"
				},
				{
					"name": "Electric grass",
					"gradient": "linear-gradient(135deg,rgb(202,248,128) 0%,rgb(113,206,126) 100%)",
					"slug": "electric-grass"
				},
				{
					"name": "Midnight",
					"gradient": "linear-gradient(135deg,rgb(2,3,129) 0%,rgb(40,116,252) 100%)",
					"slug": "midnight"
				}
			],
			"heading": true,
			"link": false,
			"palette": [
				{
					"color": "#EEEEEE",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#361C1C",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#FA6900",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#473BF0",
					"name": "Secondary",
					"slug": "secondary"
				}
			],
			"text": true
		},
		"custom": {
			"formInput": {
				"color": "var(--wp--preset--color--base)",
				"background": "var(--wp--preset--color--white)",
				"borderColor": "var(--wp--preset--color--base)"
			},
			"jetpackTestimonial": {
				"borderRadius": "50px",
				"tileBackground": "var(--wp--preset--color--primary)",
				"width": "var(--wp--style--global--wide-size)"
			}
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "80vw"
		},
		"shadow": {
			"defaultPresets": true,
			"presets": [
				{
					"name": "Natural",
					"slug": "natural",
					"shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)"
				},
				{
					"name": "Deep",
					"slug": "deep",
					"shadow": "12px 12px 50px rgba(0, 0, 0, 0.4)"
				},
				{
					"name": "Sharp",
					"slug": "sharp",
					"shadow": "6px 6px 0px rgba(0, 0, 0, 0.2)"
				},
				{
					"name": "Outlined",
					"slug": "outlined",
					"shadow": "6px 6px 0px -3px rgba(255, 255, 255, 1), 6px 6px rgba(0, 0, 0, 1)"
				},
				{
					"name": "Crisp",
					"slug": "crisp",
					"shadow": "6px 6px 0px rgba(0, 0, 0, 1)"
				}
			]
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"customSpacingSize": false,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"spacingScale": {
				"operator": "*",
				"increment": 1.5,
				"steps": 10,
				"mediumStep": 1.5,
				"unit": "rem"
			}
		},
		"typography": {
			"customFontSize": true,
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_normal_200.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_normal_600.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_normal_800.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_normal_900.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "italic",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_italic_200.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_italic_300.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_italic_400.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_italic_500.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_italic_600.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_italic_700.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "italic",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_italic_800.ttf"
							]
						},
						{
							"fontFamily": "Source Code Pro",
							"fontStyle": "italic",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/source-code-pro/source-code-pro_italic_900.ttf"
							]
						}
					],
					"fontFamily": "\"Source Code Pro\", serif",
					"name": "Source Code Pro",
					"slug": "source-code-pro"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/open-sans/open-sans_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/open-sans/open-sans_italic_500.ttf"
							]
						}
					],
					"fontFamily": "\"Open Sans\", sans-serif",
					"name": "Open Sans",
					"slug": "open-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"slug": "tiny",
					"size": "0.75rem"
				},
				{
					"name": "Small",
					"slug": "small",
					"size": "0.8rem"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1rem"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"name": "Large",
					"slug": "large",
					"size": "1.875rem"
				},
				{
					"name": "Extra Large",
					"slug": "x-large",
					"size": "2rem"
				},
				{
					"name": "Extra Extra Large",
					"slug": "xx-large",
					"size": "3.5rem"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": false,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": true
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": true
				}
			},
			"core/image": {
				"lightbox": {
					"allowEditing": true
				}
			},
			"core/list": {
				"custom": {
					"listStyles": {
						"arrow": "sssss",
						"checkbox": "sssss"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"color": true,
					"radius": true,
					"style": true,
					"width": true
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "var( --wp--preset--color--black )",
							"radius": "0",
							"style": "solid",
							"width": "3px"
						},
						"shadow": "var( --wp--preset--shadow--outlined )",
						"spacing": {
							"padding": {
								"top": "0.5rem",
								"bottom": "0.5rem",
								"left": "1.5rem",
								"right": "1.5rem"
							}
						}
					}
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"text": "var(--wp--preset--color--base)",
					"background": "var(--wp--preset--color--primary)"
				},
				"spacing": {
					"padding": "calc(0.667em + 2px) calc(1.333em + 2px)"
				},
				"typography": {
					"fontSize": "inherit",
					"fontFamily": "inherit",
					"lineHeight": "inherit",
					"textDecoration": "none"
				},
				"border": {
					"width": "0"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "3.625rem",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--source-code-pro)",
					"fontWeight": "400",
					"lineHeight": "1.4"
				}
			},
			"link": {
				"typography": {
					"textDecoration": "underline"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}			
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--50)",
			"padding": {
				"top": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)",
				"bottom": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		}
	]
}
